$(document).ready(function() {
    
    var tablez = document.getElementById("table");
    for (i = 1; i < 12; i++) { 
        var row = tablez.insertRow(i);
        var cell1 = row.insertCell(0);
        var cell2 = row.insertCell(1);
        var cell3 = row.insertCell(2);
        
       

        cell1.innerHTML =  "Töö " + i;
        cell2.innerHTML = "" + i%5;
        cell3.innerHTML = "-";
      
        
       
    }
    
    $('#table').DataTable();
} );